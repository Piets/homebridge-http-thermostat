		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"homebridge": "^1.3.5",
		"nodemon": "^2.0.13",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.3.0",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"axios": "^1.1.3"
