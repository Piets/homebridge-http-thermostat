{
	"displayName": "Homebridge HTTP Thermostat",
	"name": "@piets/homebridge-http-thermostat",
	"version": "1.0.0",
	"description": "Homebridge Plugin for custom HTTP based thermostat",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/Piets/homebridge-http-thermostat.git"
	},
	"bugs": {
		"url": "https://github.com/Piets/homebridge-http-thermostat/issues"
	},
	"engines": {
		"node": ">=14.18.1",
		"homebridge": ">=1.3.5"
	},
	"main": "dist/index.js",
	"scripts": {
